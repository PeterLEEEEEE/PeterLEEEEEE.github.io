while (top != -1) {
	temp = pop(<span class="ni">&amp;top);</span>
	printf("%c ", temp-&gt;data);
	
	if (temp-&gt;right != NULL) {
		push(temp-&gt;right, <span class="ni">&amp;top);</span>
